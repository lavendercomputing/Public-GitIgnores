# Ignore Visual Studio specific files
.vs/
*.sln
*.suo
*.opensdf
*.sdf
*.VC.db
*.VC.opendb

# Ignore compiled object files
*.slo
*.lo
*.o
*.obj

# Ignore precompiled headers
*.gch
*.pch

# Ignore compiled dynamic libraries
*.so
*.dylib
*.dll

# Ignore Fortran module files
*.mod

# Ignore compiled static libraries
*.lai
*.la
*.a
*.lib

# Ignore executables and application files
*.exe
*.out
*.app
*.ipa

# Ignore Xcode project and workspace files
*.xcodeproj
*.xcworkspace

# Ignore generated binary files
Binaries/
Plugins/*/Binaries/

# Ignore built data and precompiled assets
Build/
SourceArt/**/*.png
SourceArt/**/*.tga
*_BuiltData.uasset

# Ignore generated configuration files
Saved/

# Ignore intermediate files
Intermediate/
Plugins/*/Intermediate/

# Ignore editor cache files
DerivedDataCache/

# Whitelist specific files in the Build directory
!Build/*/
Build/*/**
!Build/*/PakBlacklist*.txt
!Build/**/*.ico
